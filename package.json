{
  "name": "node-ts-starter",
  "version": "0.3.0",
  "description": "",
  "main": "./src/app.ts",
  "scripts": {
    "format": "prettier --check \"src/**/*.ts\" --write",
    "lint": "eslint --ext \".ts,.js\" src --quiet",
    "ts-node": "ts-node src/app.ts",
    "commands": "npm run lint && npm run format && npm run ts-node",
    "monitor": "nodemon --watch src -e ts --exec \"npm run commands\"",
    "prepublish": "tsc",
    "start": "nodemon --watch src -e ts --exec \"npm run ts-node\""
  },
  "keywords": [
    "node",
    "typescript",
    "boilerplate",
    "starter"
  ],
  "author": "",
  "license": "MIT",
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": "1000"
  },
  "devDependencies": {
    "@types/node": "^12.12.7",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "dependencies": {}
}
