    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "ts-node": "^8.3.0"
  },
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/inquirer": "^6.0.3",
    "@types/node": "^12.0.10",
    "@typescript-eslint/eslint-plugin": "^1.11.1-alpha.8",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint-plugin-prettier": "^3.1.0",
    "nodemon": "^1.19.1",
    "prettier": "1.18.2",
    "typescript": "^3.5.2"
  }
